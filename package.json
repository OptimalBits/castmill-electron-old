{
  "name": "castmill-player",
  "version": "0.1.0",
  "private": true,
  "author": "OptimalBits <info@optimalbits.com>",
  "scripts": {
    "build": "yarn lint && vue-cli-service electron:build",
    "build:linux": "vue-cli-service electron:build dist --linux",
    "build:win": "vue-cli-service electron:build dist --win",
    "lint": "tslint --project tsconfig.json",
    "dev": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "publish:linux": "publish linux-armv7l --path dist_electron"
  },
  "dependencies": {
    "@types/node-fetch": "^2.1.4",
    "address": "^1.0.3",
    "auto-launch": "^5.0.5",
    "electron-log": "^2.2.17",
    "electron-simple-publisher": "^0.6.2",
    "electron-simple-updater": "^1.4.0",
    "electron-store": "^2.0.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.3.0",
    "node-wifi": "^2.0.4",
    "request": "^2.88.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vuetify": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/auto-launch": "^5.0.0",
    "@types/electron-store": "^1.3.0",
    "@vue/cli-plugin-typescript": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "electron": "^3.0.0",
    "electron-builder": "^20.38.2",
    "husky": "^1.1.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.0",
    "vue-cli-plugin-electron-builder": "^1.0.0-rc.9",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/OptimalBits/castmill-electron/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "homepage": "https://github.com/OptimalBits/castmill-electron#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "UNLICENSED",
  "main": "background.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OptimalBits/castmill-electron.git"
  }
}
